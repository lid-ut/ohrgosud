<?xml version='1.0' encoding='utf-8'?>
<widget id="ru.ohrgos" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Охраняется государством</name>
    <description>
        Охраняется государством
    </description>
    <author email="" href="http://ohrgos.ru">
        ohrgos
    </author>
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="prerendered-icon" value="true" />
    <content src="index.html" />
    <platform name="android">
        <icon density="ldpi" src="res/mipmap-ldpi/icon.png" />
        <icon density="mdpi" src="res/mipmap-mdpi/icon.png" />
        <icon density="hdpi" src="res/mipmap-hdpi/icon.png" />
        <icon density="xhdpi" src="res/mipmap-xhdpi/icon.png" />
    </platform>
    <platform name="ios">
        <icon src="res/icon.png" />
    </platform>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1.3.3" />
    <engine name="android" spec="^6.2.3" />
</widget>
